{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "lambda:UpdateFunctionCode",
        "lambda:GetFunction",
        "lambda:CreateFunction",
        "lambda:AddPermission",
        "iam:PassRole",
        "s3:PutObject",
        "s3:GetObject"
      ],
      "Resource": [
        "arn:aws:lambda:*:*:function:autosnsflow-auth",
        "arn:aws:iam::*:role/*"
      ]
    }
  ]
}


